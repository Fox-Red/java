create table timetest
(
	id int,
	datetimefield datetime,
	timestampfield timestamp
);

create table stu_with_constraint
(
	id int,
	sn int unique,
	name varchar(20) not null
);
insert into stu_with_constraint (id,sn,name) values(1,201901,'james');
create table stu_with_constraint
(
	id int primary key,
	sn int unique,
	name varchar(20) not null
);
insert into stu_with_constraint (id,sn,name) values(4,null,'james4');




